<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.isep.eleve.javaproject.controller.HomePageController">
   <children>
      <HBox prefHeight="100.0" prefWidth="200.0" />
      <VBox prefHeight="400.0" prefWidth="200.0">
         <children>
            <Label text="MENU">
               <VBox.margin>
                  <Insets left="30.0" top="10.0" />
               </VBox.margin>
            </Label>
            <Button fx:id="home" mnemonicParsing="false" onAction="#handleChangeHomePageAction" text="Home">
               <VBox.margin>
                  <Insets left="20.0" top="10.0" />
               </VBox.margin></Button>
            <Button fx:id="portfoliosInformations" mnemonicParsing="false" onAction="#handleChangePortfolioPageAction" text="Portfolios Informations">
               <VBox.margin>
                  <Insets left="20.0" top="10.0" />
               </VBox.margin></Button>
            <Button fx:id="newPortfolio" mnemonicParsing="false" onAction="#handleChangeNewPortfolioPageAction" text="New Portfolio">
               <VBox.margin>
                  <Insets left="20.0" top="10.0" />
               </VBox.margin></Button>
            <Button fx:id="assets" mnemonicParsing="false" onAction="#handleChangeAssetsPageAction" text="Assets">
               <VBox.margin>
                  <Insets left="20.0" top="10.0" />
               </VBox.margin></Button>
            <Button fx:id="buySellAssets" mnemonicParsing="false" onAction="#handleChangeBuySellAssetsPageAction" text="Buy / Sell Assets">
               <VBox.margin>
                  <Insets left="20.0" top="10.0" />
               </VBox.margin></Button>
            <Button fx:id="crypto" mnemonicParsing="false" onAction="#handleChangeCryptoPageAction" text="Crypto">
               <VBox.margin>
                  <Insets left="20.0" top="10.0" />
               </VBox.margin></Button>
            <Button fx:id="buySellCrypto" mnemonicParsing="false" onAction="#handleChangeBuySellCryptoPageAction" text="Buy / Sell Crypto">
               <VBox.margin>
                  <Insets left="20.0" top="10.0" />
               </VBox.margin></Button>
            <Button fx:id="Analysis" mnemonicParsing="false" onAction="#handleChangeAnalysisPageAction" text="Analysis">
               <VBox.margin>
                  <Insets left="20.0" top="10.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
